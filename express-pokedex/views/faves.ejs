<div id="pokes">
    <h1 class="text-center"> The Pokedex</h1>
    <ul id="fav-pokes">
        <% favorites.forEach(fave=> { %>
            <li><a href='/pokemon/<%= fave.name %>'><%= fave.name %></a></li>
            <form method='DELETE' action='/pokemon'>
                <input hidden type="text" name="name" value="<%= fave.name%>">
                <input type='submit' id="delete" value="Delete Pokemon">
            </form>
        <% }) %>
    </ul>
</div>