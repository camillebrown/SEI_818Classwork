<div class="heading">
  <h1> List of All Categories </h1>
  <p id="about">Click the links to check out which projects used these categories of tools.</p>
</div>

<div class="categories">
  <% categories.forEach(category=> { %>
    <div class="well">
        <a href="/categories/<%=category.id%>"><%=category.name%></a>
        
    </div>    
    <% }); %>
</div>